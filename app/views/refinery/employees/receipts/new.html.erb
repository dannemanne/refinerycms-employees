<h1>Add Receipt</h1>

<%= form_for @xero_receipt, url: refinery.employees_expense_claim_receipts_path(@xero_receipt.xero_expense_claim) do |f| %>

  <%= render 'form', f: f %>

  <p>
    <%= f.submit 'Save', class: 'button round' %>
    <%= f.submit 'Save & Add another', name: 'add_another', class: 'button round' %>
  </p>
<% end %>

<p><%= link_to 'Go Back', refinery.employees_expense_claim_path(@xero_receipt.xero_expense_claim) %></p>
